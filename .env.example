# OpenDart API 키 설정
# https://opendart.fss.or.kr/ 에서 발급받은 API 키를 입력하세요
API_KEY=your_opendart_api_key_here

# Google Gemini API 키 설정
# https://aistudio.google.com/app/apikey 에서 발급받은 API 키를 입력하세요
GEMINI_API_KEY=your_gemini_api_key_here

# Flask 설정
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your-secret-key-here
